{
    "name": "paynl/http-psr-serverrequest",
    "description": "",
    "type": "library",
    "keywords": [
        "request",
        "psr",
        "server",
        "http"
    ],
    "homepage": "https://bitbucket.org/paynl/http-psr-serverrequest/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Wesley de Kanter",
            "email": "wesley@pay.nl"
        }
    ],
    "require": {
        "php": "^7.4",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.96"
    },
    "autoload": {
        "psr-4": {
            "PayNL\\Http\\Psr\\ServerRequest\\": "./src/"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [

    ],
    "config": {
        "secure-http": true,
        "sort-packages": true,
        "discard-changes": true
    },
    "scripts": {
    },
    "scripts-descriptions": {
    },
    "archive": {
        "exclude": [
            "!vendor", "composer.phar"
        ]
    }
}
